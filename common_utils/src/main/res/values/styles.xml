<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="overlayWhite">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@color/white_overlay</item>
    </style>

    <style name="progressBar">
        <item name="android:indeterminateDrawable">@drawable/progress_bar</item>
        <item name="android:indeterminateDuration">1500</item>
        <item name="android:interpolator">@android:anim/linear_interpolator</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

</resources>
